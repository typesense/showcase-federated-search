{
  "name": "showcase-federated-search",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "parcel index.html --port 3000",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "typesenseServer": "docker run -i -p 8108:8108 -v/tmp/typesense-server-data/:/data typesense/typesense:0.23.0.rc56 --data-dir /data --api-key=xyz --listen-port 8108 --enable-cors",
    "indexData": "node scripts/indexData.js",
    "build": "parcel build index.html --public-url https://federated-search.typesense.org"
  },
  "engines": {
    "node": ">=14.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.1.2",
    "babel-eslint": "10.1.0",
    "eslint": "8.13.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "parcel-bundler": "1.12.5",
    "prettier": "2.6.2",
    "sass": "^1.50.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.9",
    "@popperjs/core": "^2.11.5",
    "bootstrap": "^5.1.3",
    "dotenv": "^16.0.0",
    "instantsearch.js": "^4.40.4",
    "jquery": "^3.6.0",
    "popper.js": "^1.16.1",
    "typesense": "^1.2.2",
    "typesense-instantsearch-adapter": "^2.4.0"
  }
}
